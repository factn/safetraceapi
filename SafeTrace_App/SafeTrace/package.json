{
  "name": "SafeTrace",
  "scripts": {
    "start": "react-native start --reset-cache",
    "android:reverse": "adb reverse tcp:8081 tcp:8081",
    "android": "ENVFILE=.env.dev react-native run-android",
    "ios": "ENVFILE=.env.dev react-native run-ios",
    "test": "jest --watch --coverage=false --changedSince=origin/master",
    "testDebug": "jest -o --watch --coverage=false",
    "testFinal": "jest",
    "updateSnapshots": "jest -u --coverage=false",
    "lint": "tslint --project tsconfig.json",
    "bundle:android": "react-native bundle --dev false --platform android --entry-file index.js --bundle-output ./android/app/src/main/assets/index.android.bundle --assets-dest ./android/app/src/main/res",
    "bundle:ios": "",
    "build:android:debug": "cd android && ./gradlew assembleDebug"
  },
  "jest": {
    "preset": "react-native",
    "transformIgnorePatterns": [
      "node_modules/(?!(jest-)?react-native|react-clone-referenced-element|@react-native-community|expo(nent)?|@expo(nent)?/.*|react-navigation|@react-navigation/.*|@unimodules/.*|unimodules|sentry-expo|native-base|@sentry/.*)"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json",
      "jsx",
      "node"
    ],
    "setupFiles": [
      "./tests/setup.js"
    ]
  },
